[binaries]
c = '${ARCH}-menix-mlibc-gcc'
cpp = '${ARCH}-menix-mlibc-g++'
ar = '${ARCH}-menix-mlibc-gcc-ar'
nm = '${ARCH}-menix-mlibc-nm'
objcopy = '${ARCH}-menix-mlibc-objcopy'
ranlib = '${ARCH}-menix-mlibc-ranlib'
strip = '${ARCH}-menix-mlibc-strip'
pkg-config = '${ARCH}-menix-mlibc-pkg-config'
llvm-config = '/base_dir/support/llvm-config'

[host_machine]
system = 'menix'
cpu_family = '${ARCH}'
cpu = '${ARCH}'
endian = 'little'

[built-in options]
libdir = 'lib'
