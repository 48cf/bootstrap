[binaries]
c = 'clang'
cpp = 'clang++'
ar = 'llvm-ar'
nm = 'llvm-nm'
objcopy = 'llvm-objcopy'
ranlib = 'llvm-ranlib'
strip = 'llvm-strip'
pkg-config = '${ARCH}-menix-mlibc-pkg-config'
llvm-config = '/base_dir/support/llvm-config'

[host_machine]
system = 'menix'
cpu_family = '${ARCH}'
cpu = '${ARCH}'
endian = 'little'

[built-in options]
libdir = 'lib'
